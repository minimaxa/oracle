run {
 allocate channel prmy1 type disk;
 allocate channel prmy2 type disk;
 allocate auxiliary channel stby type disk;
DUPLICATE TARGET DATABASE FOR STANDBY FROM ACTIVE DATABASE
DORECOVER SPFILE 
SET DB_UNIQUE_NAME='KIS03S'
SET CONTROL_FILES='/u01/app/oracle/oradata/KIS03/control01.ctl','/u01/app/oracle/oradata/KIS03/control02.ctl'
SET CLUSTER_DATABASE='false'
SET LOG_ARCHIVE_DEST_1='LOCATION=USE_DB_RECOVERY_FILE_DEST VALID_FOR=(ALL_LOGFILES,ALL_ROLES) DB_UNIQUE_NAME=KIS03S'
SET LOG_ARCHIVE_DEST_2='SERVICE=KIS03 ASYNC VALID_FOR=(ONLINE_LOGFILES,PRIMARY_ROLE) DB_UNIQUE_NAME=KIS03'
SET FAL_SERVER='KIS03'
SET MEMORY_TARGET='400M'
NOFILENAMECHECK;
 }

